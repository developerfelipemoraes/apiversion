{
  "Mongo": {
    "ConnectionString": "env:MONGODB_URI",
    "Database": "env:MONGO_DB"
  },
  "Jwt": {
    "Secret": "env:JWT_SECRET",
    "Issuer": "Aurovel",
    "Audience": "AurovelClients",
    "ExpiresInMinutes": 60
  },
  "Cors": {
    "AllowedOrigins": []
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [
      "FromLogContext"
    ]
  }
}
